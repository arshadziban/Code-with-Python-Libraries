    "# Importing\n",
    "import numpy as np \n",
    "\n",
    "n_arr = np.array([[10.5, 22.5, np.nan], \n",
    "                    [41, 52.5, np.nan]]) \n",
    "\n",
    "print(\"Given array:\") \n",
    "print(n_arr) \n",
    "\n",
    "print(\"\\nRemove all columns containing non-numeric elements \") \n",
    "print(n_arr[:, ~np.isnan(n_arr).any(axis=0)]) \n"
   ]
